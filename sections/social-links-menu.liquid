


<div data-section-id="{{ section.id }}"  data-section-type="header-sicial-type" class="header-sicial-type" style="">


<ul class="inline-list social-icons">
  {% if settings.social_twitter_link != blank %}
    <li>
      <a class="icon-fallback-text twitt hexagon" target="blank" href="{{ settings.social_twitter_link }}" title="{{ 'layout.footer.twitter' | t: name: shop.name, platform: 'Twitter' }}">
       
     
        <span class="fab fa-twitter" aria-hidden="true"></span>
       
      </a>
    </li>
  {% endif %}
  {% if settings.social_facebook_link != blank %}
    <li>
      <a class="icon-fallback-text fb hexagon" target="blank" href="{{ settings.social_facebook_link }}" title="{{ 'layout.footer.facebook' | t: name: shop.name, platform: 'Facebook' }}">
        <span class="fab fa-facebook" aria-hidden="true"></span>
        <span class="fallback-text">Facebook</span>
      </a>
    </li>
  {% endif %}
  {% if settings.social_pinterest_link != blank %}
    <li>
      <a class="icon-fallback-text pin hexagon" target="blank" href="{{ settings.social_pinterest_link }}" title="{{ 'layout.footer.pinterest' | t: name: shop.name, platform: 'Pinterest' }}">
        <span class="fab fa-pinterest" aria-hidden="true"></span>
        <span class="fallback-text">Pinterest</span>
      </a>
    </li>
  {% endif %}
  {% if settings.social_google_plus_link != blank %}
    <li>
      <a class="icon-fallback-text google hexagon" target="blank" href="{{ settings.social_google_plus_link }}" title="{{ 'layout.footer.google' | t: name: shop.name, platform: 'Google Plus' }}" rel="publisher">
        <span class="fab fa-google-plus-g" aria-hidden="true"></span>
        <span class="fallback-text">Google</span>
      </a>
    </li>
  {% endif %}
  {% if settings.social_instagram_link != blank %}
    <li>
      <a class="icon-fallback-text ins" target="blank" href="{{ settings.social_instagram_link }}" title="{{ 'layout.footer.instagram' | t: name: shop.name, platform: 'Instagram' }}">
        <span class="fab fa-instagram" aria-hidden="true"></span>
        <span class="fallback-text">Instagram</span>
      </a>
    </li>
  {% endif %}
  {% if settings.social_tumblr_link != blank %}
    <li>
      <a class="icon-fallback-text tumblr" target="blank" href="{{ settings.social_tumblr_link }}" title="{{ 'layout.footer.tumblr' | t: name: shop.name, platform: 'Tumblr' }}">
        <span class="fab fa-tumblr" aria-hidden="true"></span>
        <span class="fallback-text">Tumblr</span>
      </a>
    </li>
  {% endif %}
  {% if settings.social_youtube_link != blank %}
    <li>
      <a class="icon-fallback-text youtube" target="blank" href="{{ settings.social_youtube_link }}" title="{{ 'layout.footer.youtube' | t: name: shop.name, platform: 'YouTube' }}">
        <span class="fab fa-youtube" aria-hidden="true"></span>
        <span class="fallback-text">YouTube</span>
      </a>
    </li>
  {% endif %}
  {% if settings.social_vimeo_link != blank %}
    <li>
      <a class="icon-fallback-text vimeo" target="blank" href="{{ settings.social_vimeo_link }}" title="{{ 'layout.footer.vimeo' | t: name: shop.name, platform: 'Vimeo' }}">
        <span class="fab fa-vimeo" aria-hidden="true"></span>
        <span class="fallback-text">Vimeo</span>
      </a>
    </li>
  {% endif %}
  
  {% if settings.social_rss_link != blank %}
    <li>
      <a class="icon-fallback-text rss" target="blank" href="{{ settings.social_rss_link }}" title="{{ 'layout.footer.RSS' | t: name: shop.name, platform: 'RSS' }}">
        <span class="fab fa-rss" aria-hidden="true"></span>
        <span class="fallback-text">RSS</span>
      </a>
    </li>
  {% endif %}
</ul>





</div>










<style>







  .header-type-16 .header-sicial-type li a::after { background: {{section.settings.social_icon_border_clr}};}

  .header-type-16 .header-sicial-type li a {color:{{section.settings.social_icon_clr}};}

  .header-type-16 .header-sicial-type li a:hover {color:{{section.settings.social_hovericon_clr}};}
  
  
  
  
  
  .header-type-16 .header-sicial-type li a.twitt::after { background: {{section.settings.social_1_iconclr}};}
  
  .header-type-16 .header-sicial-type li a.fb::after { background: {{section.settings.social_2_iconclr}};}
  
  .header-type-16 .header-sicial-type li a.pin::after { background: {{section.settings.social_3_iconclr}};}
  
  .header-type-16 .header-sicial-type li a.google:after { background: {{section.settings.social_4_iconclr}};}
  
  .header-type-16 .header-sicial-type li a.ins::after { background: {{section.settings.social_5_iconclr}};}
  
  .header-type-16 .header-sicial-type li a.tumblr::after { background: {{section.settings.social_6_iconclr}};}
  
  .header-type-16 .header-sicial-type li a.youtube::after { background: {{section.settings.social_7_iconclr}};}
  
  .header-type-16 .header-sicial-type li a.vimeo::after { background: {{section.settings.social_8_iconclr}};}
  
  .header-type-16 .header-sicial-type li a.rss::after { background: {{section.settings.social_9_iconclr}};}
  
  
  
  
  




</style>












