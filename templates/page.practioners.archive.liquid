{% if page.metafields.accentuate.page_title != blank %}
	{% assign page_title = page.metafields.accentuate.page_title %}  
{% else %}
	{% assign page_title = page.title %}  
{% endif %}

{% section 'page-hero' %}

<div class="page-content text-center"> 
  <div class="section-header">
    <h1 class="section-header--title">{{ page_title }}</h1>
  </div>
  
  <div class="rte">
  	{% if page.metafields.accentuate.page_subtitle != blank %}   
    	<h2>{{ page.metafields.accentuate.page_subtitle }}</h2>
  	{% endif %}
  	{% if page.content != blank %}<p>{{ page.content }}</p>{% endif %}
	</div>    
</div>

{% section 'practioners-text-with-image-archive' %}