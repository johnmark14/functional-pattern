// Loaders

.ring-loader{
  display: none;
  position: relative;
  width: 24px;
  height: 24px;
}
.ring-loader div {
  box-sizing: border-box;
  display: block;
  position: absolute;
  width: 24px;
  height: 24px;
  border: 2px black solid;
  border-color: black transparent transparent transparent;
  border-radius: 50%;
  animation: ring-loader 1.2s cubic-bezier(0.5, 0, 0.5, 1) infinite;
}
.ring-loader div:nth-child(1) {
  animation-delay: -0.45s;
}
.ring-loader div:nth-child(2) {
  animation-delay: -0.3s;
}
.ring-loader div:nth-child(3) {
  animation-delay: -0.15s;
}
@keyframes ring-loader {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}




// Buttons

.btn{
  transition: none;
  border-radius: 3px;
  text-align: center;
  
  &.loading{    
    .btn-loading{
      display: block;
    }
  }
    
  
  &.btn-primary{
	color:#ffffff;
    background:{{settings.color_primary}};  
    border: 2px {{settings.color_primary}} solid; 
    
    .btn-loading div{
      border: 2px white solid;
      border-color: white transparent transparent transparent;
    }    
      
    &:active, &:focus, &:hover{

      background: radial-gradient(52.69% 52.69% at 52.69% 50%, #f7194e 0%, {{settings.color_primary}} 100%) !important;
      color: #ffffff !important; 
      border: 2px solid {{settings.color_primary}};
    }
  }
  
  &.btn-secondary{
	color:{{settings.color_primary}};
    background:#ffffff;
    border: 2px {{settings.color_primary}} solid;
    
    &:active, &:focus, &:hover{
      background: radial-gradient(52.69% 52.69% at 52.69% 50%, #f7194e 0%, {{settings.color_primary}} 100%) !important;
      color: #ffffff !important; 
      border: 2px solid {{settings.color_primary}};
    }
  }
  
  &.btn-link{
	color: #000000;
    background: transparent;
    border: 2px transparent solid;
    text-decoration: none;
    
    &:active, &:focus, &:hover{
      border: 2px transparent solid;
      text-decoration: underline;
    }
  }  
    
    
  &.btn-link-white{
	color: #FFFFFF;
    background: transparent;
    border: 2px transparent solid;
    text-decoration: none;
    
    &:active, &:focus, &:hover{
      border: 2px transparent solid;
      text-decoration: underline;
    }
  }  
  
  

  
  @media screen and (min-width: 768px) and (max-width: 1199px){
    font-size: 16px;
    padding: 15px 15px !important;
  }
  @media screen and (max-width: 767px){
    font-size: 20px;
    font-weight: 400;
    line-height: 23px;
    letter-spacing: 0.02em;
  }
}

