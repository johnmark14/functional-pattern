/* Header Sticky */
.header-sticky { 
  z-index:999;
  float:left;
  width:100%;
}

.sticky-logo img { 
  display:none; 
}

.sticky-animate {
  width:100%;
  float:left;
  position:relative;
}

.is-sticky { 
  height:auto !important;

  .header-sticky {
    width:100%;
    left:0;
    right:0;
    @include at-query ($max, $medium) {
      {% if settings.use_sticky_mobile %} 
      position:static !important; 
      {% endif %} 
    }

    .sticky-animate {
      box-shadow:0 0 20px rgba(0,0,0,0.06);      
    }
  }

  .sticky-logo {
    img {
      display:inline-block;
      float:left;
      max-width:97%;
      @include at-query ($max, $semilarge) { 
        max-width:15%;
        padding-top:10px;
        padding-right:10px;
      }
    }
  }

  .site-header__menubar { 
    position:static; 
  }

  .site-header__logo { 
    border:none;
  }

  .grid__item:nth-child(2n) { 
    float:right;
  }

  .menu-right-section { 
    position:absolute;
    right:15%;
    bottom:0;
    z-index:99;
  }

  .menu-right li a { 
    padding:16px 15px;
    font-size:16px; 
  }

  .header_top { 
    display:none;
    @include at-query ($max, $semilarge) { 
      display:inline-block !important;
    }
  }

  .header-bar__search {
    span {
      border:none;
    }

    form {
      .btn {
        position:inherit;

