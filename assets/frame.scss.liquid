/* Blog Post Types */
.blog-post {
  & + div:not(container) {
    .style1 {
      padding:0 3%;
    }
  }
  .blog-post-inner > .grid-uniform {
    margin:0 -15px;
    @include at-query ($max, $semilarge) { 
      margin:0 -10px; 
      display: flex; flex-direction: column-reverse;

    }
  }
  
/*   .article-item {
    padding:0 15px;
    margin-bottom:30px;
    @include at-query ($max, $semilarge) { 
      padding:0 10px; 
      margin-bottom:20px;
    }
  }
 */
  .blog-post-inner { 
    position:relative;
    float:left;
    width:100%;

    &:hover {
      .carousel-arrow {
        &.nav-middle {
          .prev-arrow,
          .next-arrow {
            @include opacity(1);
          }
        }
      }
    }
  }


  .article {
    float:left; 
    width:100%; 
    position:relative;

    .article-header {
      margin-bottom:10px;

      i {
        font-size:13px;
        margin-right:5px;
      }
    }
  }

  /* Style1 */
  .style1 {
    .article {       
      margin-bottom:0;
      padding:0 15px;
      @include at-query ($max, $medium) { 
        padding:0 10px; 
      }

      .article-image {
        margin-bottom:20px; 
        @include at-query($max, $semilarge) { 
          margin-bottom:0; 
        }

        a {
          display:block; 
          position:relative;          
          &:after {
            content: ''; 
            width: 390px; 
            height: 100%; 
            background: {{ settings.color_borders | color_modify: 'alpha', 0.3 }}; 
            position: absolute; 
            left: -15px; 
            top:25px; 
            z-index: -1; 
            @include transition(all 0.5s linear);
            @include at-query($max, $semilarge) { 
              width:100%; 
              left:-10px; 
              top:10px;
            }
          }
        }
      }

      .article-description {
        float:left;
        text-align:left;
        padding-top:20px;
        padding:70px 30px 0; 
        @include at-query($max, $laplarge) { 
          padding:50px 20px 0;
        } 
        @include at-query($max, $postlarge) { 
          padding:30px 0 0;
        }   
        .article-header {          

          .blog-date,
          .author {
            float:none;
            display:inline-block;
            margin-bottom:0;
          }
          .blog-date {
            margin-top:0;
            @include at-query($max, $postlarge) {
              font-size:14px;
            }
          }
        }
        .article-content {
          border-bottom:1px solid {{ settings.color_borders }};
          margin-bottom:20px;   
        }
        .article-btn {
          float:left;

          .btn {
            padding:6px 12px;
          }
        }
        .comments-count {
          font-size:14px; 
          float:right; 
          margin:6px 0 0;

          i {
            margin-right:5px;
          }
        }
      } 

      &:hover {
        .article-image {
          a {
            &:after {
              width:450px; 
              @include at-query($max, $semilarge) { 
                width:100%; 
                height:340px; 
              }
            }      
          }
        }
      }
    }
  }

  /* Style2 */
  .style2 {

    .article { 
      background:$colorBody;
      border:1px solid $colorBorder;
      @include border-radius($radius);
      overflow:hidden;      

      .article-image {
        img { 
          display:block;
        }
      }
      .article-description {
        padding:50px 40px 40px 40px;
        position:relative;

        .article-header {
          position:absolute;
          top: -20px;
          padding: 5px 20px;
          font-size: 16px;

          .blog-date,
          .author {
            float:none;
            display:inline-block;
            margin-bottom:5px;
          }
          .blog-date {
            margin-top:0;
            @include at-query($max, $postlarge) {
              font-size:14px;
            }
          }
        }

        h4 {
          margin-bottom:20px;
          @include at-query($max, $semilarge) {
            margin-bottom:12px;
          }
        }

        p {
          margin-bottom:20px;
          @include at-query($max, $semilarge) {
            margin-bottom:15px;
          }
        }
        .comments-count {
          display:none;
        }
      }

      &.content-overimage {
        .article-description {
          position:absolute;
          width:100%;
          top:0;
          left:0;
          @include opacity(0);       
        }

        &:hover {
          .article-image {
            img {
              @include opacity(.05);
              @include transition(all 0.5s linear);
            }            
          }
          .article-description {
            @include opacity(1);
            @include transition(all 0.5s linear);
          }
        }
      }
    }
  }

  /* Style3 */
  .style3 {    

    .article-item {      
      &:last-child {
        margin-bottom:0; 
      }

      .article {

        .article-image {
          overflow:hidden;

          img {
            display:block; 
            @include transform(scale(1));
            @include transition(all 0.5s linear);
          }
        }
        .article-description {
          float:left; 
          width:100%;
          padding-top:25px;

          .article-header {
            margin-bottom:8px;
            p {
              display:inline-block;
            }
          }
        }  

        &:hover {
          .article-image {
            img {
              @include transform(scale(1.1));
            }
          }
          .article-description {
            h4 a {
              color: {{ settings.color_primary_color }} !important;
            }
          }
        }
      }      
    }    

    .blog-right {
      .article-item {
        .article {
          .article-image {
            float:left; 
            width:41%;
            @include at-query($max, $small) {
              width:100%;
            }
          }
          .article-description {
            padding-top:0;
            padding-left:25px; 
            width:calc(100% - 41%);
            @include at-query($max, $small) {
              padding:25px 0 0;
              width:100%;
            }
          }
        }
      }
    }
  }  

  .blog-tag {
    margin-bottom:15px;
    i {
      display:none;
    }
    .blog-tags {
      display:inline-block;
      padding:0 12px;
    }
    .meta-sep { 
      display:none;
    }
  }

  .article-content {
    h4 {
      font-size:22px;
      margin-bottom:8px;
    }
  }

  .article-header {
    p {
      color:{{ settings.black_borders | color_modify: 'alpha', 0.5 }};
      display:inline-block;
      margin-bottom:5px;

      &.blog-date {
        margin-right:10px; 
      }
    }

    .comments {
      position:relative;

      &:before { 
        content:'';
        display:inline-block;
        background:{{ settings.black_borders | color_modify: 'alpha', 0.2 }};
        width:1px;
        height:11px;        
        margin-right:10px;        
      }

    }
  }
}

.link_text {
  display:inline-block;
  font-weight:normal;

  &:after {
    color:$colorPrimary;
    opacity:1;
  }
}


