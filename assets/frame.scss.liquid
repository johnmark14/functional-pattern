      }
    }
    .sales_tax_label {
      font-size: 16px;
    }
    form {
      float:right;
      margin:0;
      .add-cart-btn {
        cursor:pointer;
      }
    }    
    label {
      float:left;
    }
    del {
      line-height:normal;
    }
  }

  .product_left {
    float:left;
    width:100%;
  }

  &:hover {

    .product_right_tag .offer-price,
    .badge--sold-out .badge__text,
    .deal-clock {
      @include opacity(0);
    }
    .lSSlideOuter.vertical
    {
      right:0px;
    }


  }

  &.product-hover-11 {
    background:{{ settings.color_body_bg }}; 

    .product-container {
      height:auto;
      .grid-link {
        outline:none;
      }      
      .ImageOverlayCa {
        top:0;
        @include opacity(0);
      }

      .link_icon {
        width:100%;
        position:absolute;
        bottom:0;
        top:auto;
        @include transform(translate(0%, 0%));
        z-index:1;

        .product_link {

          @include opacity(0);
          display:inline-block;
          width:100%;
          height:40px;
          line-height:40px;
          margin:auto;
          text-align:center;
          background:{{ settings.color_primary_color }};
          color:{{ settings.color_button_primary_text }};
          @include at-query($max, $laplarge) {
            font-size:14px;
          }
          @include at-query($max, $medium) {
            line-height:32px;
            height:32px;
            font-size:13px;
          }

          &:hover {
            background:{{ settings.primary_hover_btn }};
            color:{{ settings.primary_hover_btn_text }};
          }
        }          
      }      

      .sizes-list {
        display:none; 
        position:static;
        width:100%;
        margin:10px 0;
      }

      .product-button {
        a,
        button {
          @include transform(scale(0));
          @include transition(all 0.4s ease-in-out);
        }
      }

      &:hover {
        .ImageOverlayCa,
        .link_icon .product_link {
          @include opacity(1);
        }
        .product-button {
          a,
          button {
            @include transform(scale(1));
            @include opacity(1);
          }
        }
      }


    }
    .product-detail {
      &.over-detail {
        text-align:center;
        position: absolute;
        bottom: 0;
        left: 80%;
        @include opacity(0);
        @include transition(all 0.5s ease);

        &:hover {
          left:0;
          @include opacity(1);
        }
      }
      .item-swatch {
        li {
          float:none;
          display:inline-block; 
        }        
      }
      .grid-link__meta {
        .grid-link__org_price,
        .grid-link__sale_price {
          float:none;
          display:inline-block;
          width:auto;
        }        
      }
