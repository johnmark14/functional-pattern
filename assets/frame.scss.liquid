      padding:20px 0;
    } 
  }

  .top-countdown-bar-content { 
    text-align:center;
    @include flexbox();
    @include justify-content(center);
    @include align-items(center);
    @include flex-wrap(wrap);


    h2 { 
      font-size:50px;
      line-height:normal;
      margin:0 0 60px;
      font-family: $bodyFontStack;
      @include at-query ($max, $medium) { 
        font-size:38px;
        margin:0 0 30px;
      }
      @include at-query ($max, $small) { 
        font-size:24px;
      } 
    }

    h3 { 
      font-size:20px;
      text-transform:uppercase;
      font-family: $bodyFontStack;
      font-weight:300;
      line-height:normal;
      margin:0;
    }

    p { 
      font-size:$baseFontSize;

      line-height:normal;
      margin:0 0 0px;
      float:left;
      text-align:left;
      @include at-query ($max, $small) {
        float:none;
        text-align:center;

      }
      @include at-query ($max, $postlarge) {
        margin:0;
      }


      -webkit-box-flex: 0;
      -ms-flex: 0 0 25%;
      flex: 0 0 25%;
      max-width: 25%;


      @include at-query ($max, $small) {
        -webkit-box-flex: 0;
        -ms-flex: 0 0 100%;
        flex: 0 0 100%;
        max-width: 100%;
        margin-bottom:10px;
      }


    }

    .deal-clock
    {
      -webkit-box-flex: 0;
      -ms-flex: 0 0 60%;
      flex: 0 0 60%;
      max-width: 60%;


      @include at-query ($max, $small) {
        -webkit-box-flex: 0;
        -ms-flex: 0 0 100%;
        flex: 0 0 100%;
        max-width: 100%;
        margin-bottom:10px;
      }
    }

    .lof-clock-timer-detail { 

      @include at-query ($max, $postlarge) { 
        margin:0 0 15px;
      }

      ul { 
        padding:0;

        li {
          display:inline-block;
          margin:0 12px;
          font-size:24px;

          line-height:normal; 
          position:relative;
          @include at-query ($min, $prepostLarge) { 
            border:1px solid {{ settings.black_borders }};
            @include border-radius(50%);
            width:80px;
            height:80px;
          } 
          @include at-query ($max, $small) { 
            font-size:16px;  
          }

          &:after { 
            content:":";
            float:left;
            position:absolute;
            top:50%;
            right:-16px;
            @include transform(translateY(-50%));
            font-weight:normal;
          }

          &:last-child {
            &:after {
              content:none;
              background:none;
            }
          }

          b { 
            display:block;
            margin-top:10px;
          }

          span {
            float:left;
            width:100%;
            font-size:$baseFontSize;

          }
        }    
      }
    }

    .deal-content { 

      position:relative;
      float:right;
      text-align:right;
      @include at-query ($max, $small) {
        float:none;
        text-align:center;

      }
      -webkit-box-flex: 0;
      -ms-flex: 0 0 15%;
      flex: 0 0 15%;
      max-width: 15%;

      @include at-query ($max, $small) {
        -webkit-box-flex: 0;
        -ms-flex: 0 0 100%;
        flex: 0 0 100%;
        max-width: 100%;

      }


      h4 { 
        @include opacity(0.2);
        font-family: $bodyFontStack;
        font-size:155px;
        line-height:normal; 
        margin:0;
        @include at-query ($max, $medium) { 
          font-size:100px;
        }
        @include at-query ($max, $small) { 
          font-size:50px;
        }
      }  
    }

    .deal-btn .btn {
      padding:12px 25px; 
      /* @include border-radius(1px); */
      font-size:14px;
      letter-spacing:0.5px;
    }
  }   

  .close { 
    background:{{ settings.black_borders }};
    color:{{ settings.color_borders }};
    position: absolute;
    top: 0; 
    right: 0;
    width:20px;
    height:20px;
    line-height:18px;
    text-align:center;

    &:hover {
      background:{{ settings.color_primary_color }};
    }

    svg {
      width:14px;
      height:14px;
    }
  }
}


