  padding-bottom: 100px;
  width: 440px;
  @include calc(max-width, '100vw - 40px');
  @include translateX(100%);
  padding: 16px 20px 10px;


  .cart-icon {
    position: relative;
    padding-right: 10px;
    width: 65px;

    svg {
      stroke: rgba(255, 255, 255, 0);
      width: 22px;
      height: 23px;
    }

    .cartCount {
      position: absolute;
      top: 7px;
      right: -1px;
      min-width: 23px;
      min-height: 23px;
      text-align: center;
      padding: 2px 0px 0 1px;
      @include border-radius(50%);
      z-index: 2;
      font-weight: 500;
      font-size:12px;
      background:{{ settings.color_body_bg }};border:1px solid $colorBorder;
    }
  }


  .mini-products-list {
    @include calc(max-height, '100% - 270px');
  }

  .cart-title .close,
  {
    font-size:12px;
    text-transform:uppercase;
    @include flexbox();
    @include justify-content(flex-end);
    @include align-items(center);
    font-weight:normal;

    margin-bottom: 0px;

    svg{
      width: 14px;  
    }
  }

  .cart-title .close span,
  {
    margin-left:5px;
    padding-top:1px
  }



  .cart-title h5,
  {

    text-transform:uppercase;
    letter-spacing:.05em;
    font-size:13px;

    border-bottom:1px solid $colorBorder;
    padding-bottom:17px;
    margin-bottom:0
  }

}


.cart-show  #slidedown-cart {
  @include translateX(0);
}


@media (max-width: 1199px) {
  .cart-show  #slidedown-cart
  {
    @include transform(none);

  }
  #slidedown-cart
  {
    overflow-y: scroll;
  }


}

.cart-show  #slidedown-cart {
  @include translateX(0);
}

.cart-show .wrap-overlay { display:block;}

.wrap-overlay {
  position: fixed;
  background:rgba({{ settings.overlay_bg }},0.8);
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
