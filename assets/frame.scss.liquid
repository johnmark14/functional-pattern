    .fbt-image-item {
      padding: 0 10px;
      text-align: center;
      @include transition(all .3s ease);

      &:not(.isChecked) {
        .item-image {
          @include opacity(.45);
        }
      }

      &+.fbt-image-item {
        position: relative;

        &:before {
          position: absolute;
          content: '+';
          z-index: 2;
          font-size: 18px;
          left: 0px;
          top: 50%;
          @include translate(-50%, -50%);
        }
      }
