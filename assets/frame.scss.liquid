.icon-nav .icon-line {
  display: block;
  width: 16px;
  height: 3px;
  margin: 1px 0;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  background-color: {{ settings.link_color }};
}
.icon-nav .icon-line:before, .icon-nav .icon-line:after {
  background-color: {{ settings.link_color }};
  display: block;
  width: 16px;
  height: 3px;
  margin: 1px 0;
  position: absolute;
  left: 0;
  right: 0;
  content: "";

  @include transition(transform 0.3s);
  @include transition-delay(0.2s);

}

.icon-nav {
  border: none;
  outline: none;
  padding: 0;
  display: block;
  width: 24px;
  height: 24px;
  position: relative;


  .icon-line {
    display: block;
    width: 22px;
    height: 3px;
    margin: 1px 0;
    position: absolute;
    left: 0;
    right: 0;
    top: 1px;
    background-color: {{ settings.link_color }};

    &:before,
    &:after {
      background-color: {{ settings.link_color }};
      display: block;
      width: 22px;
      height: 3px;
      margin: 1px 0;
      position: absolute;
      left: 0;
      right: 0;
      content: "";
      @include transition(transform .3s);
      @include transition-delay(.2s);
    }

    &:before {
      top: 7px;
    }

    &:after {
      top: 15px;
    }
  }
}

.close-menu-mb {
  position: fixed;
  left: 0;
  right: auto;
  top: 0;
  visibility: hidden;
  webkit-transition: all 0s ease-in-out;
  -moz-transition: all 0s ease-in-out;
  -ms-transition: all 0s ease-in-out;
  -o-transition: all 0s ease-in-out;
  transition: all 0s ease-in-out;
  
  &.menu-open {
    display: block;
    background-color: #000000;
    z-index: 3333;
    width: 53px;
    height: 53px;

    left: 100%;
    visibility: visible; 
    -webkit-transform: translateX(-50px);
    transform: translateX(-50px);   

    .icon-line {
      background: none;

      &:before,
      &:after {
        background: $colorBtnPrimaryText;
        left: 18px;
      }

      &:before {
        @include rotate(45);
        top: 20px;
      }

      &:after {
        @include rotate(-45);
        top: 20px;
      }
    }
  }
}






