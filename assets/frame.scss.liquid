      &:last-child{margin-bottom:0;}

    }
    code
    {
      font-family:monospace;
    }

  }
  label
  {
    float:left;
    text-align: center;
    margin: 0 0px  0;
    border:2px solid $colorBorder;
    @include at-query($max, $semilarge)
    {
      margin: 0 0px  0;
    }
  }
  input:checked + label,label:hover
  {
    border:2px solid {{ settings.color_primary_color }};
  }
  .header
  {
    float:left;
    width:100%;
    color:{{ settings.product_label_color }};
  }

