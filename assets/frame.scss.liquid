/* Header Bar Social Icons */
.header-bar { 
  float:left;
  width:100%;
  padding:5px 15%;
  @include at-query ($max, $postlarge) { 
    padding:0 10px;
  }
  @include at-query ($max, $semilarge) { 
    padding:0;
  }
  @include at-query ($max, $medium) { 
    border:none;
  }

  .social-icons {
    li {
      display:inline-block;
      float:left;
      @include at-query ($max, $medium) { 
        float:none;
        display:inline-block;
        text-align:center;
      }

      a {
        display:inline-block;
        float:left;
        font-size:14px;
        padding:0;
        @include at-query ($max, $medium) { 
          float:none; 
        }
      }
    }
