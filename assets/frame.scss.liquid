/* ----- Owl Carousel General Style ----- */
.owl-carousel {
  padding:0;

  .owl-item { 
    padding:0;

    li {
      list-style:none;
    }
  }  

  &.owl-theme { 
    overflow:hidden;

    .owl-dots {
      .owl-dot {
        span {
          width: 10px;
          height: 10px;
          margin: 5px 7px;
          background: {{ settings.color_primary }};
          border:1px solid {{settings.color_primary_border}};
          display: block;
          -webkit-backface-visibility: visible;	
          @include transition(opacity .2s ease);	
          @include border-radius(30px);
        }

        &.active,
        &:hover {
          span {
            background:{{ settings.primary_hover_btn  }};
            border-color: {{settings.color_primary_border}};
            color:{{ settings.color_primary_color }};
          }          
        }
      }
    }
  }  
}  
/* Owl Carousel Arrow */  
