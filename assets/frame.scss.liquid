/*================ Module | Notes and Form Feedback ================*/
.note,
.errors {
  display:inline-block;
  width:100%;
  @include border-radius($radius);
  padding: 15px;
  margin-bottom: $gutter / 2;
  border: 1px solid transparent;
  font-size:{{ settings.type_base_size }}; 
  text-align: left;

  ul,
  ol { 
    margin-top: 0; 
    margin-bottom: 0;
  }

  li:last-child { 
    margin-bottom: 0;
  }

  p { 
    margin-bottom: 0;
  }
}
.note { 
  border-color: $colorBorder;
}

.errors {
  ul {

    margin-left: 10px;

    li {
      margin-bottom:5px;
    }
  }
}

.form-success { 
  color: $successGreen;
  background-color: $successGreenBg;
  border-color: $successGreen;

  a { 
    color: $successGreen;
    text-decoration: underline;

    &:hover { 
      text-decoration: none;
    } 
  }
}

.form-error,
.errors {
  color: $errorRed; 
  background-color: $errorRedBg;
  border-color: $errorRed;

  a { 
    color: $errorRed;
    text-decoration: underline;

    &:hover { 
      text-decoration: none;
    }
  }
}

/*================ Module | Pagination ================*/
.pagination { 
  margin-bottom: 1em;
}
.pagination-custom, .pagination-blog { 
  @include border-radius($radius);
  display: inline-block;
  width:100%;
  padding-left: 0;
  margin-top: 30px;
  @include at-query ($max, $small) { 
    margin-top:20px;
  }

  > li {
    display: inline-block;

    a.enable-arrow { 
      background:{{ settings.color_primary  }};
      color: $colorBtnPrimaryText;

      &:hover { 
        background:{{ settings.primary_hover_btn  }};
        color: {{ settings.primary_hover_btn_text }};
      }
    }

    > a,
    > span {        
      position: relative;
      float: left;
      padding: 0px 5px;
      margin-left: -1px;
      line-height: 40px;
      min-width:40px;
      text-decoration: none;
      background:{{ settings.color_primary  }};
      color: $colorBtnPrimaryText;
      margin-right:2px;
      @include border-radius($radius);

      &:hover {
        background:{{ settings.primary_hover_btn  }};
        color: {{ settings.primary_hover_btn_text }};
      }
    }

    &:first-child {
      > a,
      > span {
        margin-left: 0;
      }
    }

    &.active {
      > a,
      > span,
      > a:hover,
      > span:hover, 
      > a:focus,
      > span:focus {
        background:{{ settings.primary_hover_btn  }};
        color: {{ settings.primary_hover_btn_text }}; 
        cursor: default;
        z-index: 2;       
      }
    }

    &.disabled {
      > a,
      > span,
      > a:hover, 
      > a:focus {
        cursor: not-allowed;      
      }
    }
  }
}
.pagination-custom-sm, .pagination-blog-sm {
  li {
    > a,
    > span { 
      padding: 8px 13px;
      font-size: em(12px);
    }
  }
}
.pagination-custom-lg, .pagination-blog-lg  {
  li {
    > a,
    > span { 
      padding: 10px 16px;
      font-size: em(18px);
    }
  }
}


