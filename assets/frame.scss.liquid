    }    
  }
  .reveal {
    .swipe {
      @include at-query ($max, $small) { 
        left:0;
        right:0;
      }
    }
  }

  {% if settings.enable_color_swatch %}
  .item-swatch {
    list-style:none;
    padding:0;
    float:left;
    width:100%;
    margin:20px 0 0 !important;  
    letter-spacing:-.33em; 
    line-height:0;
    li {
      float:none;
      display:inline-block;
      letter-spacing:normal; 
      text-align:center; 
      vertical-align:middle; 
      position:relative;
      margin:0 3px 0 0;
      a {
        width:30px;
        height:30px;
        display:inline-block;
        cursor:pointer;
        line-height:31px;
        text-align:center;
        font-size:12px;
        font-weight:500;
      }
      label {
        width:50px; 
        height:50px;
        @include border-radius(50%); 
        margin:0; 
        cursor:pointer;
        background-position:center; 
        background-repeat:no-repeat; 
        border:2px solid transparent;
        @include at-query ($max, $medium) { 
          width:28px;
          height:28px;
        }

        &.active {
          @include box-shadow(0, 0, 1px, 2px, $colorBorder);
        }
        &:hover,
        &.active {          
          border-color: {{ settings.color_primary_color }};
        }
      }
      .tooltip {
        text-align:center; 
        background:{{ settings.primary_hover_btn }}; 
        color:{{ settings.primary_hover_btn_text }};
        bottom:100%; 
        padding:10px 7px;
        display:block; 
        position:absolute;
        z-index:2;
        min-width:44px;
        left:50%;
        font-size:11px;
        font-weight:500;
        letter-spacing:normal;
        pointer-events:none;
        margin-bottom:10px;
        @include opacity(0);
        visibility:hidden; 
        @include box-shadow(2px, 2px, 15px, rgba(0,0,0,0.1));
        @include transition(all 0.25s ease-out);        
        @include transform(translateX(-50%));      
        &:after {
          border-left:5px solid transparent; 
          border-right:5px solid transparent; 
          border-top:5px solid {{ settings.black_borders }}; 
          bottom:-5px;
          content:" "; 
          height:0;
          left:50%;
          @include transform(translateX(-50%));      
          position:absolute;
          width:0;
        }
      } 
      &:first-of-type {
        margin-left:0
      }
      &:last-of-type {
        margin-right:0
      }

      &:hover {
        .tooltip {
          @include opacity(1);
          visibility:visible;
          pointer-events:auto;
        }

        {% if settings.overlay_use %}
        .ImageOverlayCa
        {
          background:rgba({{ settings.product_overlay }}, 0.5);
        }
        {% endif %}

      }
    }
    &.swatch_count {
      position:absolute;
      top:-40px;
      left:0;
      right:0;
      li {
        label {
          cursor:pointer;
          margin:0;
          line-height:40px;
          width:60px;
          height:40px;
          background:#f2f2f2;
        }
      }
    }
    &.color_swatch_Value {
      label {
        width:16px;
        height:16px;
        @include border-radius(50%);
        float:left;
        cursor:pointer;
      }
    }
  }
  {% endif %}
  .sizes-list {
    float:left;
    padding:0;
    margin:10px 0 0 !important;
  }

  .product-container {
    float:left;
    width:100%;
    @include border-radius({{ settings.products_border_radius }}px {{ settings.products_border_radius }}px 0 0);
    position:relative;
    overflow:hidden;
    {% if settings.products_border_use %} 
    border:{{ settings.products_border_width }}px solid {{ settings.products_border }};
    {% endif %}    
    {% if settings.show_img_height %}    
    height:{{ settings.products_img_height }}px;    
    @include at-query ($max, $semilarge) { 
      height:{{ settings.products_img_height_tab }}px;
    }
    {% endif %}
    @include at-query ($max, $medium) { 
      width:100%;
    }

    .ImageOverlayCa { top: 0;height: 100%;}

    img {
      {% if settings.show_img_height %}
      max-height:100%;
      position:absolute;
      top:50%;
      left:0;
      right:0;
      @include transform(translateY(-50%));
      {% endif %}
    }



    $badgeSize: 60px;
    .badge 
    { 
      @include border-radius($radius);
      float:left;
      position:absolute;
      color: {{ settings.sale_button_text }};
      margin:0px;
      text-align: center;
      top:5px;
      left:5px;
      font-size: em(12px);
      line-height: 1.1;
      z-index: 1;
    }
    .badge--sold-out 
    {
      top: 0;
      left:0;
      margin-top: 0;
      margin-left: 0;
      z-index:2;
      .badge__text 
      {
        @include transition(all 0.4s ease-in-out);
        background-color: {{ settings.color_sold }};
        color: {{ settings.color_sold_text }};
      }

    }
    .badge__text 
    {
      @include border-radius($radius);
      display: inline-block;
      font-size:12px;
      padding:1px 10px;
      line-height:normal;
      text-transform:uppercase;
      @include at-query ($max, $semilarge) 
      {
        font-size: 11px;
      }
    }
    .badge__text--small 
    {
      font-size: 8px;
      padding-top: 0;
    }

    .badge--sale {
      @include border-radius($radius);
      background:{{ settings.sale_bg }};
      float:left;
      margin:0;      
      @include transition(width 0.1s cubic-bezier(0.86, 0, 0.07, 1) 0.05s);
      .badge__text {
        background:{{ settings.sale_bg }};
        float:left;
        overflow:hidden;
        padding-left:10px;
        padding-right:10px;
        @include border-radius({{ settings.btn_border_radius }}px);
        @include transition-delay(0s);

        -webkit-transition-duration: 0.3s, 0.25s, 0.25s;
        -moz-transition-duration: 0.3s, 0.25s, 0.25s;
        -ms-transition-duration: 0.3s, 0.25s, 0.25s;
        -o-transition-duration: 0.3s, 0.25s, 0.25s;
        transition-duration: 0.3s, 0.25s, 0.25s;
        -webkit-transition-property: opacity, padding, width;
        -moz-transition-property: opacity, padding, width;
        -ms-transition-property: opacity, padding, width; 
        -o-transition-property: opacity, padding, width; 
        transition-property: opacity, padding, width;
        -webkit-transition-timing-function: cubic-bezier(0.86, 0, 0.07, 1);
        -moz-transition-timing-function: cubic-bezier(0.86, 0, 0.07, 1);
        -ms-transition-timing-function: cubic-bezier(0.86, 0, 0.07, 1); 
        -o-transition-timing-function: cubic-bezier(0.86, 0, 0.07, 1);
        transition-timing-function: cubic-bezier(0.86, 0, 0.07, 1);
      }      
      i {
        position:absolute;
        right:12px;
      }
    }

    .deal-lable { 
      display:none;
    }
    .deal-clock {
      display:inline-block;
      text-align:center;
      width:100%;
      position:absolute;
      bottom:0;
      left:0;
      right:0;
      margin:auto;
      z-index:1;

      ul {
        padding:0;
        list-style:none;
        display:inline-block;
        text-align:center;
        width:100%;
        margin:0;

        li {
          display:inline-block;
          text-align:center;
          border:none;
          border-right:1px solid $colorBorder;
          background:{{ settings.dealbtn }};
          color:{{ settings.dealbtn_text }};
          width:25%;
          line-height:normal;
          padding:10px;
          @include at-query($max, $laplarge) {
            font-size:10px;
            padding:7px 0;
          }
          @include at-query($max, $medium) { 
            font-size:14px; 
          }

          &:last-child {
            border-right:none;
          }

          span {
            text-transform:uppercase;
            font-size:10px;
            display:block;
            @include at-query($max, $laplarge) {
              font-size:8px;
            }
            @include at-query($max, $medium) { 
              font-size:10px;
            }
          }
          i {
            display:block;
          }
        }
      }
    }

    .ImageWrapper {
      float:left;
      width:100%;
      overflow:hidden;
    }

    .ImageOverlayCa {
      height: 100%; 
      left: 0;
      top: 0%;
      @include opacity(0);
      width: 100%;
      position: absolute;
      z-index:1;
      @include transition(all 0.5s);
    }

    .product-button {
      @include opacity(0);
      display:inline-block;
      text-align:center;
      margin:0;
      position: absolute;
      top: 5%;
      right:7%;
      @include transform(translateY(0%));      
      @include transition(all 0.5s);
      z-index:2;

      form {
        display:block;
        margin:0;
        width:initial;
        padding:0;
        float:none;
      }

      .button-row-1,
      .button-row-2 {
        @include at-query ($max, $medium) { 
          display:inline-block;
        }
      }

      .loading {
        display:inline-block;
        width:100%;
        margin:auto;
        text-align:center;
        position:relative;
        padding:0;
        background:none;
      }

      button {
        background:none;
        padding:0;
        @include transition(all 0.4s ease-in-out); 

        a {
          .quick-view-text,
          .compare {
            @include at-query ($max, $medium) {
              display:none;
            }
          }
        }
      }
      .wishlist {
        margin:0;
      }
      .add-to-wishlist {         
        a {
          display:block;
        }
        b {
          position: absolute;
          left: 15px;
          top: 10px;
          font-size: 12px;
        }
        .tooltip-label {
          display:none;
        }
      }
      .add-in-wishlist-js span {
        display:none;
      }
      span {
        cursor:no-drop;
      }

      a,
      button,
      span {
        background:{{ settings.products_icon_bg }}; 
        @include box-shadow(0, 0px, 15px, rgba(0,0,0,0.15));      
        @include border-radius(50%);
        color: {{ settings.products_icon_clr }};
        cursor:pointer;
        font-size:14px;
        display:block;
        padding:0;
        margin:4px 0;
        width:40px;
        height:40px;
        line-height:40px;
        text-align:center;
        position: relative;
        @include transition(all 0.4s ease-in-out);
        @include at-query($max, $laplarge) { 
          width:30px;
          height:30px;
          line-height:30px;
          font-size:14px;
          margin-right:0px;
        }
        @include at-query($max, $medium) { 
          width:25px;
          height:25px;
          line-height:25px;
          font-size:10px;
          margin-right:5px;
        }               
      }
      a,
      button {
        &:hover {
          background:{{ settings.products_icon_hover_bg }};
          color: {{ settings.products_icon_hover_clr }};
        }
      } 
    }

    .grid__image {
      float:left;
    }

    &:hover {

      .reveal .swipe
      {
        z-index:9;
        @include opacity(1);
      }
      .reveal .hidden-feature_img 
      { 
        visibility: visible;
        @include opacity(1);
        top:50%;
        @include transform(translate(0%, -50%));
      }
      .product-additional img 
      { 
        visibility: hidden;
      }


      .product-button 
      {
        @include opacity(1);    
      }
    }

    .reveal .hidden-feature_img 
    {
      position: absolute;
      top: 0px; 
      visibility: hidden;
      left: 0px;
      right:0;
      margin:auto;
      width: auto; 
      @include opacity(0);
      @include transition(opacity 0.4s ease-in-out);
/*       @include transform(translateX(100%)); */
/*       @include transition(all 0.4s ease-in-out); */
    }

/*     .product-additional img { 
      {% if settings.show_img_height %}
      @include transform(translate(0 ,-50%));
      {% endif %}
      @include transition(all 0.4s linear);
    } */

    .reveal .swipe{
      display:block!important;
      visibility:visible!important;
    }   

  }

  .reveal .hidden
