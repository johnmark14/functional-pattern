/* ----- Sidebar Widgets ----- */

.widget {
  float:left;
  width:100%;
  margin-bottom:40px;

  h4 {
    position:relative;
    color:{{ settings.sidebar_text }};
    @include border-radius({{ settings.btn_border_radius }}px);
    padding-bottom:10px;
    font-size:1.15em;

    border-bottom:1px solid {{ settings.color_borders }}; 
  }

  ul {
    float:left;
    width:100%;
    list-style:none;
    padding:0;
  }
}
.sidebar {  
  @include at-query ($max, $medium) {
    padding-right:0px;
    display:none;
  }

  &.product_sidebar,
  &.collection_sidebar {
    float:left;
    width:100%;
  }
  .widget {   
    h4 {
      a.clear {
        background:{{ settings.color_body_bg }};
        font-size:14px; 
        float:right;
        margin:0;
        padding:0;
        position:absolute;
        top:0;
        right:0;
        width:30px;
        height:30px;
        line-height:30px;
        text-align:center;
        color:{{ settings.sidebar_text }};
        border:none;
      }
    }

    .carousel-block {
      float:left;
      width:100%;
      position:relative; 

      .sidabar_nav {
        a {
          width:35px;
          height:35px;
          line-height:32px;
          padding:0;
          margin:0;
          position:absolute;
          top:38%;
          @include opacity(0);
          @include transform(translateY(-50%));

          &.prev {
            left:10px;
          }
          &.next {
            right:10px;
          }
        }
      }

      &:hover {
        .sidabar_nav {
          a {
            @include opacity(1);
          }
        }
      }
    }
  }

  /* Product Category */
  .widget_product_categories {
    ul {
      li {
        float:left;
        width:100%;
        position:relative;
        padding:6px 0;
        @include transition(all 0.4s ease-in-out);
        a {
          float:left;
          width:90%;
          padding:0;
        }
        span {
          float:right;
          font-size:12px;
        }

        ul.children {
          background:none;
          padding:10px 0 5px 30px;
          margin:0;
          border:none;
          list-style:disc;

          li {
            a {
              font-size:$baseFontSize - 4;
            }
          }
        }
      }

    }

    .dt-sc-toggle {
      float:right;
      text-align:center;
      cursor:pointer;
      &:before {
        cursor:pointer;
        font-size: 10px;
        content:"\f067"; 
        font-family: "Font Awesome 5 Free";
        font-weight:bold;
      }
      &.active {
        &:before {
          content:"\f068";          
        }
      }

      &:hover {
        &:before {
          color:{{ settings.color_primary_color }};
        }
      }
    }
  }

  /* Product Hot Deal */
  .widget_hot_deals {
    .ul {
      border:none !important;
      li {
        a.thumb { 
          position:relative;
          float:left;
          width:100%;
          text-align:center;
          img {
            margin:0 auto;
          }
        }
        .lof-clock-timer-detail-single {
          margin:0;
          float:left;
          display:inline-block;
          text-align:center;
          list-style:none;
          padding:0 !important;
          position:absolute;
          bottom:0;
          left:0;
          right:0;
          z-index:99;

          li {
            display:inline-block;
            width:23%;
            height:52px;
            padding:10px 0;
            margin:0 1%;
            font-size:20px;

            line-height:1;
            position:relative;
            @include at-query ($max, $postlarge) {
              font-size:12px;
              height:45px;
            }
            span {
              float:left;
              width:100%;
              font-weight:300;
              text-transform:uppercase;
              font-size:12px;
              line-height:1;
              @include at-query ($max, $postlarge) {
                font-size:10px;
              }
            }
          }
        }

        .products {
          border:none;
          .product-detail {
            .product_left {
              width:100%;
            }
            .grid-link__meta {
              margin-bottom:5px;
            }
          }
        }
      }
    }
  }

  /* Product Bestseller */
  .widget_top_rated_products {
    ul {
      li {	
        float:left;
        width:100%;
        padding:12px 0;
        border-bottom:1px dashed $colorBorder;

        .top_product_count {
          display:none;
          float:left;
          margin-right:20px;
          font-size:50px;
          @include at-query ($max, $postlarge) { 
            font-size:36px;
            margin-right:15px;
          }
          @include at-query ($max, $semilarge) { 
            font-size:46px;
          }
          @include at-query ($max, $medium) { 
            display:none;
          }
        }
        .top-products-detail {
          background:none;
          float:left;
          width:65%;
          padding:0;
          text-align:left;

          .spr-badge {
            font-size:11px !important;
            padding:0;
          }

          .product_price {
            .grid-link__org_price,
            del {
              .money {
                font-size:14px;
              }
            }            
          }
        }
        .products-container {
          float:right;
          width:25%;
          margin-left:5%;
        }

        &:last-child {
          border:none;
          padding-bottom:0;
        }
      }
    }
  }



  /* ----- Widget Recent Articles ----- */
  .widget_recent_article {
    ul {
      li {
        float:left;
        padding:12px 0;
        border-bottom:1px dashed $colorBorder;
        float:left;
        width:100%;

        &:last-child {
          border-bottom:none;
        }

        .article-image {
          float:left;
          width:30%;
        }

        .article-description {
          float:left;
          width:65%;
          margin-left:5%;

          h6 {

            margin-bottom:7px;
            text-transform:inherit;
          }

          p {
            margin:0;
            line-height:normal;
          }
        }
      }
    }
  }

  /* ----- Widget Tags ----- */
  .widget_categories {
    ul {
      li {
        float:left;
        width:100%;
        padding:6px 0;
      }
    }
  }

  /* ----- Widget Instagram ----- */
  .widget_instagram {
    .lush-instagram {

      .item {      
        float:left;
        width:33.33%;
        position:relative;  

        img {
          display:block;
          height:auto;
        }

        .info {
          display:none;
          top: 0; 
          left: 0;
          right: 0;
          height: 100%;
          @include opacity(0);
          pointer-events: none;
          position:absolute;
          text-align:center;
          @include transition(all 0.4s ease-in-out);

          span {
            display:inline-block;
            width:50px;
            height:45px;
            line-height:45px;
            position:absolute;
            top:50%;
            right:50%;
            margin: 0 5px;
            @include transform(translateY(-50%));

            &.likes {
              left:50%;
              right:auto;
            }

            i {
              display:inline-block;
              margin-right:3px;
            }
          }
        }

        &:hover {
          .info {
            @include opacity(1);
          }
        }
      }
    }
  }  

  &.collection_sidebar {    
    .widget {
      &.widget_colors,
      &.widget_tag_filter,
      &.widget_colors {
        h4 {
          padding:0 25px 12px 0;
          span {
            cursor:pointer;
            display:block;
            &:before {
              font-size: 12px; 
              position: absolute;
              content: "\f078";
              right: 0;
              top:40%;
              font-family: "Font Awesome 5 Free";
              font-weight:bold; 
              @include transition(all 0.5s ease);
              @include transform(translateY(-50%));
            }        
          } 
        }
      }

      &.click {
        h4 span {
          &:before {
            @include transform(translateY(-50%) rotate(180deg));
          }
        }
      }
    }
  }
}

