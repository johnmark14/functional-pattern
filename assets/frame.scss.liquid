.slick-slider {

  &.more-view-vertical
  {


    .slick-prev 
    { 
      @include at-query($min, $large) {
        top:-25px;
        left:0;
        right:0;
        width:auto;
        height:auto;
        margin:0 auto;
        text-align:center;
        color: {{ settings.color_body_text }};
        @include transform(translateY(0%)translateX(50%)rotate(90deg));

      }
    }

    .slick-prev 
    {

      @include at-query($max, $medium) 
      {

        top:50%;
        left:-15px;
        margin:0 auto;
        text-align:center;
        color: {{ settings.color_body_text }};
        @include transform(translateY(-50%));

      }
    }

  }


  .slick-prev 
  {

    top:50%;
    left:-15px;
    margin:0 auto;
    text-align:center;
    color: {{ settings.color_body_text }};
    @include transform(translateY(-50%));

  }

  &.more-view-vertical
  {


    .slick-next 
    { 
      @include at-query($min, $large) {
        color: {{ settings.color_body_text }};
        margin:0 auto;
        text-align:center;
        top:auto;
        bottom:-15px;
        left:0;
        right:0;
        width:auto;
        height:auto;

        @include transform(translateY(0%)translateX(50%)rotate(90deg));
      }

    }




    .slick-next 
    { 
      @include at-query($max, $medium) 
      {
        color: {{ settings.color_body_text }};
        margin:0 auto;
        text-align:center;
        top:50%;
        right:-15px;
        @include transform(translateY(-50%));


      }

    }

  }

  svg
  {
    width:12px;
    height:12px;
    @include at-query ($max, $postlarge) 
    {
      width:10px;
      height:10px;

    }
  }


  .slick-next 
  { 
    color: {{ settings.color_body_text }};
    margin:0 auto;
    text-align:center;
    top:50%;
    right:-15px;
    @include transform(translateY(-50%));


  }





  .slick-next:before,.slick-prev:before 
  { 
    content:none; 
  } 

}



.slick-prev:hover,
.slick-prev:focus,
.slick-next:hover,
.slick-next:focus
{
  color: {{ settings.color_primary_color }};
  outline: none;
