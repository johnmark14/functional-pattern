/* Slick Slider Carousel Arrows */
.carousel-arrow,
.slick-slider {
  .slick-arrow {
    display:inline-block; 
    background:{{ settings.color_primary  }};
    color: $colorBtnPrimaryText;
    border-radius:50%; 
    padding:0;
    cursor:pointer;
    height: 35px;
    width: 35px;
    line-height: 35px;
    font-size: 14px; 
    margin-right:3px;
    position:absolute;  
    text-align:center;
    z-index:1;

    &:before {
      display:none;
    }

    i {
      font-size:14px; 
      margin:0;
    }
    &:hover,
    &:focus {
      background:{{ settings.primary_hover_btn  }};
      color: {{ settings.primary_hover_btn_text }}; 
    }

    &.prev-arrow {
      margin-right:8px;
    }
    &.next-arrow {


    } 
    &.slick-arrow.slick-disabled {
      cursor:default;
      opacity:0.2;

      &:hover {
        background:none;
        border-color:{{ settings.black_borders }};

        i {
          color:{{ settings.black_borders }};
        }
      }
    }
  }   
  &.nav-top-left,
  &.nav-top-right {
    position:absolute;
    top:-50px;        
    width:auto;

    .slick-arrow {
      border-radius:0;
    }
  }
  &.nav-top-left {
    left:0;
  }
  &.nav-top-right {
    right:0;
  }
  &.nav-middle {
    position:absolute;
    top:42%;
    @include transform(translateY(-50%));
    width:100%;
    margin:0;
    z-index:1;

    .prev-arrow,
    .next-arrow { 
      position:absolute;
      opacity:0;
      margin:0;
    }
    .prev-arrow {
      left:-15px;
    }
    .next-arrow {
      right:-15px;
    }
  }
  &.nav-bottom {
    text-align:center;        
  }
  &.nav-bottom-left {
    width:auto;          
  }
  &.nav-bottom-right {
    float:right;
    width:auto;          
  }
  &.nav-top-left,
  &.nav-top-right,
  &.nav-bottom,
  &.nav-bottom-left,
  &.nav-bottom-right,
  &.over-image {
    .slick-arrow {
      position:relative;
    }
  }
  button {
    &.slick-arrow {
      margin:0;
    }
  }
}


