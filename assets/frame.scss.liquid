/* ----- Blockquote ----- */
blockquote { 
  position:relative;

  &:before { 
    content: ',,'; 
    font-family:Arial, Helvetica, sans-serif;
    font-size:60px;
    left: -23px;
    letter-spacing:-7px;
    position: absolute;
    top: -20px;
    width: 70px;
    text-align: center;
    @include transform(rotate(180deg));
  }

  q::before {
    content: none;
  }
}


