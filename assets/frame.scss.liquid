  /* Tag Filter */
  .sidebar-tag {
    li {
      input[type=checkbox] {
        display:none;

        & + label {          
          padding:7px 0 7px 28px;
          margin:0;
          font-weight: normal;
          @include transition(all 0.4s ease-in-out);

          &:before {
            content:''; 
            width:18px;
            height:18px;
            border-radius:50%;
            border:2px solid {{ settings.color_borders }}; 
            display:inline-block;
            position:absolute;
            top:0;
            bottom:0;
            margin:auto;
          }

          &:after {
            display:none;
          }

          &:hover {
            cursor: pointer;
          }
        }     

        &:checked {

          & + label:before,
          & + a span:before {
            content:'\2714'; 
            color:{{ settings.color_body_bg }}; 
            font-size:11px;
            font-weight:normal;
            text-align:center;
            line-height:14px;
          }
          & + label {
            cursor: pointer;

            &:before {
              background:{{ settings.color_primary_color }}; 
              border-color:{{ settings.color_primary_color }};
            }
          }
          & + a span:before {
            display:inherit;
            line-height:18px;
          }
        }

      }

      &:hover {
        input[type="checkbox"] + label {
          color:{{ settings.color_primary_color }};
        }
      }
    }

    /* Color Filter */
    &.color {      
      ul {
        list-style:none;
        float:left;
        width:100%;
        padding:0;
        li {
          display:inline-block;
          padding:0;
          margin:0;
          a {
            display:inline-block; 
            margin:0; 
            border:2px solid {{ settings.color_borders }}; 
            padding:3px; 
            border-radius:50%;

