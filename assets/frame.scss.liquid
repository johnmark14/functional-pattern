/* Swatches Styles */

.swatch{
  float:left;
  width:100%;
  margin:0 0 10px;
  .header
  {
    margin:0px 0 10px;

  }
  input
  {
    display:none;
  }

  .swatch-element {
    float:left;
    -webkit-transform:translateZ();
    -webkit-font-smoothing:antialiased;
    position:relative;
    margin-bottom:10px;
    margin-right:10px;

    &.available
    {
      margin-right:5px;
    }
    &.color
    {

      background:none !important;
      border:none;
      &.white input:checked + label:before
      {
        color: $colorTextBody;
      }
      &.available
      {
        box-shadow:inherit;
        vertical-align:middle;
      }
      input:checked+label.white-tick:before
      {
        color: $colorTextBody;
      }
      label

      {
        width:30px;
        height:30px;
        background-size: cover;
        @include border-radius(50%);
        overflow:hidden;
        padding:0;
        @include transition(all 0.4s ease-in-out);
      }
      &.soldout input:checked + label.white-tick:before
      {
        content:none;
      }

    }
    &.soldout
    {
      margin-right:10px;
      label
      {
        display: block;
        cursor: not-allowed;
        @include opacity(0.6);
        border-color: #e4e4e4;
        &:before {
          position: absolute;
          content: "";
          left: 50%;
          top: -5px;
          bottom: 0;
          border-left: 1px solid;
          border-color: #828282;
          @include transform(rotate(45deg));
        }
      }
      input:checked + label.white-tick:before
      {
        content:none;
      }

    }


    &:hover .tooltip
    {
      @include opacity(1);
      visibility:visible;
      @include transform(translateY(0% ));

    }

    .bgImg
    {
      display: block;
      width: 100%;
      height: 100%;
      background-repeat: no-repeat;
      background-position: center;
      background-size: cover;
    }
    label
    {
      padding:0px 10px;
      line-height:25px;
      @include at-query($max, $semilarge)
      {
        padding:0 10px;
      }
      cursor:pointer;
    }
  }

  .tooltip
  {
    text-align:center;
    bottom:100%;
    padding:2px;
    font-size:12px;
    position:absolute;
    width:60px;
    margin-bottom:15px;
    @include opacity(0);
    visibility:hidden;
    @include transform(translateY(10px));
    @include transition(all .25s ease-out);
    @include box-shadow(2px, 2px, 15px, 0 rgba(0, 0, 0, 0.1));
    z-index:10000;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    box-sizing:border-box;
    left:-18px;
    &:before
    {
      bottom:-20px;
      content:" ";
      display:block;
      height:20px;
      left:0;
      right:0;
      position:absolute;
      width:100%;
    }
    &:after
    {
      border-top:solid {{ settings.black_borders }} 7px;
      border-left:solid transparent 7px;
      border-right:solid transparent 7px;
      bottom:-7px;
      content:" ";
      height:0;
      left:0%;
      right:0;
      margin:auto;
      position:absolute;
      width:0;
    }


  }

  .tooltip
  {
    background:{{ settings.primary_hover_btn }}; 
    color:{{ settings.primary_hover_btn_text }};
    {% if settings.color_tooltip %}
    display:block;
    {% else %}
    display:none;
    {% endif %}}
  .selector-wrapper:after
  {
    background-color:none;
  }

  &.error
  {
    background-color:#00305f!important;
    color:#333!important;
    padding:1em;
    @include border-radius(50%);
    p
