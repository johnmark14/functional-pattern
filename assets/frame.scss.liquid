  @include at-query ($max, $medium) { 
    width:100%;
    float:none;
    display:inline-block;
    text-align:center;
  } 

  span {
    padding:0;
  }

  form {
    margin:0;
    @include at-query ($max, $medium) { 
      width:85%;
      display:inline-block;
      text-align:center;
      margin:auto;
      border:1px solid $colorBorder;
    }
    @include at-query ($max, $small) { 
      width:100%;
    }

    .header-bar__search-input {
      border:none;
      float:left;
      width:85%;
      display:inline-block;
      margin:0;
      padding:14px 10px;
      background:transparent;
      min-height:50px;
      @include at-query ($max, $postlarge) { 
        width:78%;
      }
      @include at-query ($max, $semilarge) {
        width:75%;
      }
      @include at-query ($max, $medium) { 
        width:87%;
        border:none;
      }
      @include at-query ($max, $small) { 
        width:75%;
      }      
    }

    .btn {
      padding:28px 17px !important;
      font-size:{{ settings.type_base_size }};
      font-weight:normal;
      background:transparent;
      border:none;
      border-left:none;
      @include border-radius(0);
      display:inline-block;
      padding:33px 20px;
      min-height:50px;
      @include transition(all 0.4s ease-in-out);
      @include at-query ($max, $postlarge) { 
        padding:33px 5px;
        font-size:13px; 
      }
      @include at-query ($max, $medium) { 
        float:right;
        border:none;
        border-left:1px solid $colorBorder;
      }

      span {
        border:none;
      }

      &:hover {
        color:{{ settings.color_primary_color }};

        span {
          border:none;
        }
      }
    }
