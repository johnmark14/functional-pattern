/* ----- Tool Bar ----- */  
.toolbar {
  background-color: rgba(0,0,0,0.05);
  border:1px solid $colorBorder;
  padding: 9px 9px 9px 15px;
  margin-bottom: 30px;    
  @include flexbox();
  @include justify-content(space-between);
  @include align-items(center);
  @include flex-wrap(wrap);
  @include at-query($max, $medium) {
    padding:15px;
  }

  @include at-query($max, $postlarge) { 
    position: relative; 

    & .sidebar-label { position: absolute; right: 0; left: auto; bottom: calc(100% + 10px);

      button.tag-fillter  {
        display: flex;
        align-items: center; 
        svg { margin-right: 5px; width: 18px; }

      }

    }

  }

  .dropdown-menu
  { 
    position: absolute;
    top: 100%;
    left: 0;
    z-index: 100;
    display: none;
    float: left;
    padding: 15px 20px; 
    margin: 0px 0 0; 
    font-size: 14px;
    list-style: none; 
    background-color: {{ settings.color_body_bg }}; 
    background-clip: padding-box;
    border:1px solid {{ settings.color_borders }};
    @include border-radius(0 0 {{ settings.products_border_radius }}px {{ settings.products_border_radius }}px);
    @include box-shadow(0, 6px, 12px, rgba(0,0,0,0.175));    
  }

  .grid__item.right {
    @include flexbox();
    @include justify-content(flex-end);
    @include at-query($max, $medium) {
      @include justify-content(space-between);
      width:100%;
    }

    @include at-query($max, $small) {
      @include justify-content(center);
      @include align-items(center);
      flex-direction:column;
      width:100%;
    }
  }

  .toolbar-col {
    @include flexbox();
    @include align-items(center);

    label {
      font-size:14px;

      margin: 0 10px 0 0;            
      color: $colorLink;
      position: relative;
      top: 1px;
      float:left;
      letter-spacing:0.3px;
      text-transform:uppercase;

      @include at-query($max, $small) {
        width:110px;
      }

    }

    .label-tab {            
      padding:5px 10px;           
      overflow: hidden;
      text-overflow: ellipsis;
      -webkit-line-clamp: 1;
      -webkit-box-orient: vertical;
      cursor: pointer;
      background:$colorBody;
      @include flexbox();
      @include justify-content(space-between);
      @include align-items(center);
    }

    .icon-dropdown {            
      font-size: 10px;
      padding-left: 8px;
    }

    .dropdown-menu {
      padding: 0;
      margin: 2px 0;
      right: 0;          
      color: $colorBody;
      min-width: 100%;

      li {
        * {
          display: block;
          padding: 4px 14px 3px 15px;
          cursor: pointer;
          white-space: nowrap;
          overflow: hidden;
          text-overflow: ellipsis;
          color: $colorTextBody;
        }

        &.active,
        &:hover {
          * {
            background:{{ settings.primary_hover_btn }};
            color: {{settings.primary_hover_btn_text }};
          }
        }
      }
    }

    .sorting-section {
      background:$colorBody; 
    }
    .limited-view {
      min-width: 45px;
      position: relative; 
    }
    .limited-view,
    .sorting-section {
      @include at-query($max, $small) { 
        min-width:160px;

      }

      &.open {
        .dropdown-menu {
          @include border-radius(0);
          @include box-shadow(0, 0, 20px, rgba(0,0,0,.05));
          display:block;
          left:auto;
          text-align:left;
        }
      }
    }
  }

  .filters-toolbar__limited-view {
    padding-right: 15px;
    @include at-query($max, $small) { 
      padding:0;
      margin-bottom:12px;
    }        
  }

  .filters-toolbar__sortby {
    padding-left: 15px;

    .filter-sortby {
      min-width: 149px;
      position: relative;
    }
  }

  .view-mode {
    padding:0;
    position:relative;
    display:inline-block;
    @include at-query($max, $medium) 
    { 
      display:none
    }

    .icon-mode {
      min-width: 19px;
      height: 26px;
      float:left;
      margin-left: 2px;
      position: relative;
      cursor: pointer;
      vertical-align:middle;
      border:1px solid $colorBorder;

      &:first-of-type {
        margin-left: 0;
      }

      &:before {
        border:1px solid #dcdcdc;
        content: "";
        top: 3px;
        position: absolute;
        left: 3px;
      }

      &.icon-mode-list,
      &.grid-3 {
        width: 26px;
      }

      &.icon-mode-list {
        &:before {
          width: 18px;
          height: 4px;
          box-shadow: 0px 7px 0 #dcdcdc, 0px 14px 0 #dcdcdc;
        }
      }

      &.icon-mode-grid {
        &:before {
          width: 4px;
          height: 18px;
        }
      }

      &.grid-2 {
        width: 19px;

        &:before {
          box-shadow: 7px 0 0 #dcdcdc;
        }
      }

      &.grid-3 {
        &:before {
          box-shadow: 7px 0 0 #dcdcdc, 14px 0 0 #dcdcdc;
        }
      }

      &.grid-4 {
        width: 33px;

        &:before {
          box-shadow: 7px 0 0 #dcdcdc, 14px 0 0 #dcdcdc, 21px 0 0 #dcdcdc;
        }
      }

      &.grid-5 {
        width: 40px;

        &:before {
          box-shadow: 7px 0 0 #dcdcdc, 14px 0 0 #dcdcdc, 21px 0 0 #dcdcdc, 28px 0 0 #dcdcdc;
        }
      }

      &.active {
        border-color: $color_slick_arrow;

        &:before {
          background-color: $color_slick_arrow;
          border-color: $color_slick_arrow;
        }

        &.icon-mode-list {
          &:before {
            box-shadow: 0px 7px 0 $color_slick_arrow, 0px 14px 0 $color_slick_arrow;
          }
        }

        &.grid-2 {
          &:before {
            box-shadow: 7px 0 0 $color_slick_arrow;
          }
        }

        &.grid-3 {
          &:before {
            box-shadow: 7px 0 0 $color_slick_arrow, 14px 0 0 $color_slick_arrow;
          }
        }

        &.grid-4 {
          &:before {
