      .image-overlay {
        position: absolute;
        @include opacity(0);
        display: block;
        bottom: 0;
        left: 0;
        right: 0;
        width: 100%;  
        height: 100%;
        @include transition(all 0.5s);
        overflow:hidden;

        .links {
          position: absolute;
          left: 0; 
          right: 0;
          top: -7px;
          bottom: 0;
          margin: auto;  
          width: 120px;
          height: 45px;

          a {
            display: inline-block;
            height: 45px;
            width: 45px;
            @include border-radius(100%);
            font-size: 16px;
            line-height: 40px;
            margin: 0 5px;
            position: relative;
            bottom: 0;
            z-index: 1;
            text-align: center;
            @include transition(all 0.6s ease);

            &:first-child {
              right: 100%;
            }

            &:last-child {
              left: 100%;
            }

            i {
              line-height: 40px;
            }
          }
        }
      }

      .image-overlay-details {
        left: 0; 
        bottom: -100px;
        @include opacity(0);
        overflow:hidden;
        right: inherit;
        width: 100%;
        text-align: center;
        padding: 20px 10px; 
        border: 0;
        position: absolute;
        @include transition(all 0.4s ease-in-out);
        @include at-query ($max, $postlarge) { 
          padding: 10px;
        }

        h4 {
          font-size:18px;

          margin:0;
          @include at-query ($max, $postlarge) { 
            font-size:14px;
          }
        }

        p {
          margin:5px 0 0;
        }
      }

      &:hover {
        .image-overlay {
          @include opacity(1);

          .links a {
            bottom: 90%;

            &:first-child {
              right: 0px;
            }
            &:last-child {
              left: 0px;
            }
          }
        }

        .image-overlay-details {
          bottom: 0;
          @include opacity(1);
        }
      }
    }
  }
}
