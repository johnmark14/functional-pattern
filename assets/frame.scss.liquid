  .ovrly09 .overlay {height: 100%; left: -195%; width: 155%; position: absolute; top: 0; -webkit-transform: skewX(-36deg);-moz-transform: skewX(-36deg); -ms-transform: skewX(-36deg); -o-transform: skewX(-36deg);transform: skewX(-36deg); -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
