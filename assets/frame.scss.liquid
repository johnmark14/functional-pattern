/* ----- Slick Slider General Style ----- */
.slick-slider { 
  padding:0;
  position:relative;
  float:left;
  width:100%;

  .slick-list {
    float:left;
    width:100%; 
  }
  .slick-slide {
    outline:none;

    img {
      display:block;
      margin: auto;
    }
  }
  .slick-arrow {
    height:60px;
    line-height:60px;
    @include transition(all 0.3s ease-in-out);
  }
  .slick-dots {
    @include at-query($max, $semilarge) { 
      bottom:-10px !important;
    }

    li {
      width:auto;
      height:auto;

      button { 
        background:{{ settings.color_body_bg }};
        border:1px solid black;
        @include border-radius(100%);
        width:8px;
        height:8px;
        padding:0;
        text-indent:-9999px;
        margin:0;

        &:before { 
          content:none;
        }
      }

      &.slick-active button,
      button:hover,
      button:focus {
        border-color:{{ settings.black_borders }};
        background:{{ settings.black_borders }};
        color: {{ settings.primary_hover_btn_text }};
      }
    }
  }
}


