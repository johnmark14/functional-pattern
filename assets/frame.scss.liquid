// Modal Card Grids



/* The Modal (background) */
.modal.modal-grid {
  display: none;
  position: fixed; 
  left: 0;
  width: 100%; 
  height: fit-content;
  overflow: inherit;
  margin: auto;
  max-height: 800px;  
  z-index: 1030;

  &:after{
    position: fixed;
    content: "";
    display: table;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    background-color: #000000;
    opacity: 0.7;
  }
  @media screen and (min-width: 1400px){
    max-width: 60%;
  }
  @media screen and (min-width: 1024px) and (max-width: 1399px){
    max-width: 70%;
  }
  @media screen and (min-width: 968px) and (max-width: 1023px){
    max-width: 80%;
  }
  @media screen and (min-width: 768px) and (max-width: 967px){
    max-width: 80%;
  }
  @media screen and (min-width: 568px) and (max-width: 767px){
    max-width: 90%;
  }  
  @media screen and (max-width: 567px){
    max-height: 80%;
    overflow-y: scroll; 
    padding: 0 20px;
  }


  /* Modal Content/Box */
  .modal-content {
    position: relative;
    background-color: #fefefe;  
    border: none;  
    float: none;
    border-radius: 10px;
    width: 100%;
    overflow: hidden;

    .modal-content-inner{    
      display: flex;
      align-items: center;

     
      @media only screen and (max-width: 767px){
        flex-direction: column;

        .grid__item{
          width: 100%;
        }
      }
    }
    
    
    .modal-avater{
      height: 500px;
      width: 500px;

      
      @media screen and (min-width: 768px) and (max-width: 1199px){
        height: 400px;
        width: 400px;
      }
      
      .slick-list{
        height: 100% !important; 
      }

      video{
        object-fit: cover; 
      }
    }

    .modal-practitioners-inner{   
      overflow-y: scroll;
      padding: 30px 30px 40px 30px;

      &:after{                    
        content: "";
        position: absolute;
        bottom: 0;
        height: 70px;
        width: 100%;
        background-image: linear-gradient(transparent, white);                  
      }  
    }
    
    .video-mp4{
      max-height: 100%;

      video {
        max-width: 100%;
      }      
    } 
    

    @media only screen and (min-width: 1678px){
/*       .video-mp4, */
      .modal-practitioners-inner{
	      max-height: 520px;
      }
    }
    @media only screen and (min-width: 1440px) and (max-width: 1677px){
/*       .video-mp4, */
      .modal-practitioners-inner{
	      max-height: 420px;
      }
    }
    @media only screen and (min-width: 1200px) and (max-width: 1439px){
/*       .video-mp4, */
      .modal-practitioners-inner{
	      max-height: 440px;
      }
    }   
    @media only screen and (min-width: 968px) and (max-width: 1199px){
/*       .video-mp4,       */
      .modal-practitioners-inner{
	      max-height: 340px;
      }
    }      
    @media only screen and (min-width: 768px) and (max-width: 967px){
/*       .video-mp4, */
      .modal-practitioners-inner{
	      max-height: 340px;
      }
    }
   
    @media only screen and (max-width: 767px){
      .modal-practitioners-inner{
	      padding: 60px 20px;
      }
    }    



    
    .avatar{
      object-fit: cover;
      display: inherit;
      width: 100%;
      border-radius: 10px 0 0 10px;

      @media only screen and (min-width: 1678px){
        height: 570px;
      }
      @media only screen and (min-width: 1440px) and (max-width: 1677px){
        height: 430px;
      }
      @media only screen and (min-width: 1200px) and (max-width: 1439px){
        height: 420px;
      }
      @media only screen and (min-width: 1024px) and (max-width: 1199px){
        height: 360px;
      }
      @media only screen and (min-width: 968px) and (max-width: 1023px){
        height: 380px;
      }
      @media only screen and (min-width: 768px) and (max-width: 967px){
        height: 310px;
      }
      @media only screen and (min-width: 568px) and (max-width: 767px){
        height: 250px;
      }
      @media only screen and (min-width: 414px) and (max-width: 567px){
        height: 370px;
      }
      @media only screen and (min-width: 375px) and (max-width: 413px){
        height: 330px;
      }
      @media only screen and (max-width: 374px){
        height: 275px;
      }    
    }
    /* The Close Button */
    .close {
      z-index: 999;
      background-color: transparent;
      color: #fff;
      border: none;
      font-size: 32px;
      position: absolute;

      @media only screen and (min-width: 968px){
        right: -80px;        
        top: -80px;
      }
      @media only screen and (min-width: 768px) and (max-width: 967px){
        right: -30px;        
        top: -30px;
      }
      @media only screen and (min-width: 568px) and (max-width: 767px){
        right: -5px;        
        top: -50px;
      }
      @media only screen and (max-width: 567px){
        right: 0px;        
        top: 0px;
        width: 30px;
      }        
    }
    .close:hover,
    .close:focus {
      color: black;
      text-decoration: none;
      cursor: pointer;
    } 
    .loading .lds-ring {
      display: inline-block;
    } 
  }
}



